{
  "name": "lol-analytics-viewer",
  "version": "0.1.0",
  "description": "Automatically opens relevant LoL Analytics pages based on current draft status in League of Legends",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "lol-analytics-viewer": "./dist/cli/index.js"
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "cli": "tsx src/cli/index.ts",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write ."
  },
  "keywords": [
    "league-of-legends",
    "lol",
    "analytics",
    "champion-select",
    "draft",
    "lcu-api"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ws": "^8.18.0",
    "playwright": "^1.48.0",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/ws": "^8.5.13",
    "@types/node": "^22.9.0",
    "@types/jest": "^29.5.14",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
